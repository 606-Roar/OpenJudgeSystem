@using OJS.Web.Areas.Contests.ViewModels
@model IEnumerable<ContestViewModel>

@{
    ViewBag.Title = "Състезания с позволен " + ViewBag.SubmissionType;
}

<h2>
    @ViewBag.Title
</h2>
<div class="row">
    <div class="col-md-5">
        @foreach (var contest in Model)
        {
            <div>
                <a href="@ContestsHelper.GetUrl(contest.Id, contest.Name)">@contest.Name</a>
            </div>
        }
    </div>
</div>